# - Config file for the LIBBITCOIN_SERVER package
#
#  LIBBITCOIN_SERVER_INCLUDE_DIRS - include directories for LIBBITCOIN_SERVER
#  LIBBITCOIN_SERVER_LIBRARIES    - libraries to link against

get_filename_component(LIBBITCOIN_SERVER_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(LIBBITCOIN_SERVER_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

include("${LIBBITCOIN_SERVER_CMAKE_DIR}/libbitcoin-serverTargets.cmake")
set(LIBBITCOIN_SERVER_LIBRARIES libbitcoin-server)

find_package(libbitcoin-node 3.0.0 REQUIRED)
find_package(libbitcoin-protocol 3.0.0 REQUIRED)
